{
  "name": "vscode-fluent-bit",
  "displayName": "vscode-fluent-bit",
  "description": "fluent-bit language support for Visual Studio Code",
  "version": "0.1.1",
  "publisher": "sh-cho",
  "icon": "./images/extension-icon.png",
  "engines": {
    "vscode": "^1.14.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/sh-cho/fluent-bit-lsp/clients/vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/sh-cho/fluent-bit-lsp.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b -w"
  },
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "fluent-bit",
        "aliases": [
          "fluent-bit conf",
          "fluent-bit"
        ],
        "extensions": [
          ".conf"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/fluent-bit.svg",
          "dark": "./images/fluent-bit.svg"
        }
      }
    ]
  },
  "packageManager": "pnpm@8.4.0",
  "pnpm": {
    "packageExtensions": {
      "tree-sitter-fluentbit": {
        "optionalDependencies": {
          "node-gyp": "*"
        }
      }
    }
  },
  "dependencies": {
    "tree-sitter": "^0.21.1",
    "tree-sitter-fluentbit": "0.1.0-alpha.1",
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.5",
    "@types/vscode": "^1.88.0",
    "typescript": "^5.4.4"
  }
}
