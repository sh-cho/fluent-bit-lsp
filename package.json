{
  "name": "vscode-fluent-bit",
  "displayName": "vscode-fluent-bit",
  "description": "fluent-bit conf file language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sh-cho/vscode-fluent-bit.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "fluent-bit",
      "aliases": ["fluent-bit conf", "fluent-bit"],
      "extensions": [".conf"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./images/fluent-bit.svg",
        "dark": "./images/fluent-bit.svg"
      }
    }],
    "grammars": [{
      "language": "fluent-bit",
      "scopeName": "source.fluent-bit",
      "path": "./syntaxes/fluent-bit.tmLanguage.json"
    }]
  },
  "packageManager": "pnpm@8.4.0"
}
